{
  "name": "ai-resume-agent",
  "version": "2.0.0",
  "private": true,
  "description": "AI-powered resume creation and career guidance platform",
  "author": "AI Resume Agent Team",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:functions\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:functions": "cd apps/functions && npm run dev",
    "build": "npm run build:packages && npm run build:apps",
    "build:web": "npm run build:packages && cd apps/web && npm run build",
    "build:functions": "npm run build:packages && cd apps/functions && npm run build",
    "build:packages": "npm run build:agent-core && npm run build:ui-bridge && npm run build:data-connectors && npm run build:job-recommender && npm run build:career-advisor && npm run build:interview-simulator",
    "build:apps": "npm run build:web && npm run build:functions",
    "build:agent-core": "cd packages/agent-core && npm run build",
    "build:ui-bridge": "cd packages/ui-bridge && npm run build",
    "build:data-connectors": "cd packages/data-connectors && npm run build",
    "build:job-recommender": "cd packages/job-recommender && npm run build",
    "build:career-advisor": "cd packages/career-advisor && npm run build",
    "build:interview-simulator": "cd packages/interview-simulator && npm run build",
    "clean": "npm run clean:web && npm run clean:functions && npm run clean:packages",
    "clean:web": "cd apps/web && rm -rf dist node_modules/.vite",
    "clean:functions": "cd apps/functions && rm -rf dist .netlify",
    "clean:packages": "npm run clean:agent-core && npm run clean:ui-bridge && npm run clean:data-connectors && npm run clean:job-recommender && npm run clean:career-advisor && npm run clean:interview-simulator",
    "clean:agent-core": "cd packages/agent-core && rm -rf dist lib",
    "clean:ui-bridge": "cd packages/ui-bridge && rm -rf dist lib",
    "clean:data-connectors": "cd packages/data-connectors && rm -rf dist lib",
    "clean:job-recommender": "cd packages/job-recommender && rm -rf dist lib",
    "clean:career-advisor": "cd packages/career-advisor && rm -rf dist lib",
    "clean:interview-simulator": "cd packages/interview-simulator && rm -rf dist lib",
    "test": "npm run test:packages && npm run test:apps",
    "test:web": "cd apps/web && npm run test",
    "test:functions": "cd apps/functions && npm run test",
    "test:packages": "npm run test:agent-core && npm run test:ui-bridge && npm run test:data-connectors && npm run test:job-recommender",
    "test:apps": "npm run test:web && npm run test:functions",
    "test:agent-core": "cd packages/agent-core && npm run test",
    "test:ui-bridge": "cd packages/ui-bridge && npm run test",
    "test:data-connectors": "cd packages/data-connectors && npm run test",
    "test:job-recommender": "cd packages/job-recommender && npm run test",
    "test:career-advisor": "cd packages/career-advisor && npm run test",
    "test:interview-simulator": "cd packages/interview-simulator && npm run test",
    "type-check": "npm run type-check:packages && npm run type-check:apps",
    "type-check:web": "cd apps/web && npx tsc --noEmit",
    "type-check:functions": "cd apps/functions && npx tsc --noEmit",
    "type-check:packages": "npm run type-check:agent-core && npm run type-check:ui-bridge && npm run type-check:data-connectors && npm run type-check:job-recommender",
    "type-check:apps": "npm run type-check:web && npm run type-check:functions",
    "type-check:agent-core": "cd packages/agent-core && npx tsc --noEmit",
    "type-check:ui-bridge": "cd packages/ui-bridge && npx tsc --noEmit",
    "type-check:data-connectors": "cd packages/data-connectors && npx tsc --noEmit",
    "type-check:job-recommender": "cd packages/job-recommender && npx tsc --noEmit",
    "type-check:career-advisor": "cd packages/career-advisor && npx tsc --noEmit",
    "type-check:interview-simulator": "cd packages/interview-simulator && npx tsc --noEmit",
    "lint": "npm run lint:packages && npm run lint:apps",
    "lint:web": "cd apps/web && npm run lint",
    "lint:functions": "cd apps/functions && npm run lint",
    "lint:packages": "npm run lint:agent-core && npm run lint:ui-bridge && npm run lint:data-connectors && npm run lint:job-recommender",
    "lint:apps": "npm run lint:web && npm run lint:functions",
    "lint:agent-core": "cd packages/agent-core && npm run lint",
    "lint:ui-bridge": "cd packages/ui-bridge && npm run lint",
    "lint:data-connectors": "cd packages/data-connectors && npm run lint",
    "lint:job-recommender": "cd packages/job-recommender && npm run lint",
    "lint:career-advisor": "cd packages/career-advisor && npm run lint",
    "lint:interview-simulator": "cd packages/interview-simulator && npm run lint",
    "lint:fix": "npm run lint:fix:packages && npm run lint:fix:apps",
    "lint:fix:packages": "npm run lint:fix:agent-core && npm run lint:fix:ui-bridge && npm run lint:fix:data-connectors && npm run lint:fix:job-recommender",
    "lint:fix:apps": "npm run lint:fix:web && npm run lint:fix:functions",
    "lint:fix:web": "cd apps/web && npm run lint:fix",
    "lint:fix:functions": "cd apps/functions && npm run lint:fix",
    "lint:fix:agent-core": "cd packages/agent-core && npm run lint:fix",
    "lint:fix:ui-bridge": "cd packages/ui-bridge && npm run lint:fix",
    "lint:fix:data-connectors": "cd packages/data-connectors && npm run lint:fix",
    "lint:fix:job-recommender": "cd packages/job-recommender && npm run lint:fix",
    "lint:fix:career-advisor": "cd packages/career-advisor && npm run lint:fix",
    "lint:fix:interview-simulator": "cd packages/interview-simulator && npm run lint:fix",
    "db:setup": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:seed": "supabase db reset --linked && psql $DATABASE_URL -f supabase/seed.sql",
    "db:generate-types": "supabase gen types typescript --linked > packages/ui-bridge/src/types/database.ts",
    "db:status": "supabase status",
    "db:logs": "supabase logs",
    "deploy": "npm run build && npm run deploy:functions && npm run deploy:web",
    "deploy:web": "cd apps/web && npm run deploy",
    "deploy:functions": "cd apps/functions && npm run deploy",
    "deploy:staging": "npm run build && netlify deploy --dir=apps/web/dist --alias=staging",
    "deploy:production": "npm run build && netlify deploy --prod --dir=apps/web/dist",
    "start": "cd apps/web && npm run preview",
    "analyze": "npm run analyze:web && npm run analyze:functions",
    "analyze:web": "cd apps/web && npm run analyze",
    "analyze:functions": "cd apps/functions && npm run analyze",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md,css}\"",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "postinstall": "npm run build:packages",
    "update-deps": "npx npm-check-updates -u && npm install",
    "check-deps": "npx depcheck",
    "security:audit": "npm audit && npm run security:audit:packages",
    "security:audit:packages": "npm run security:audit:agent-core && npm run security:audit:ui-bridge",
    "security:audit:agent-core": "cd packages/agent-core && npm audit",
    "security:audit:ui-bridge": "cd packages/ui-bridge && npm audit",
    "performance:test": "cd apps/web && npm run test:performance",
    "e2e:test": "cd apps/web && npm run test:e2e",
    "storybook": "cd apps/web && npm run storybook",
    "docs:build": "typedoc --out docs packages/*/src/index.ts",
    "docs:serve": "serve docs",
    "health-check": "node scripts/health-check.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@supabase/cli": "^1.127.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^8.2.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.12",
    "prettier": "^3.1.1",
    "serve": "^14.2.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@10.2.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-resume-agent.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/ai-resume-agent/issues"
  },
  "homepage": "https://ai-resume-agent.com",
  "keywords": [
    "ai",
    "resume",
    "career",
    "job-search",
    "typescript",
    "react",
    "supabase",
    "openai",
    "netlify"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "import"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "always"
        }
      ]
    }
  }
}
